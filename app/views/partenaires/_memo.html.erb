  <div id="memolist">
  <ul class='p-0'>
    <% current_user.memos.each do |memo| %>
      <li data-memoid="<%=memo.id%>" class="mini-card">
        <%= link_to memo, method: :delete, remote: true do %>
      <i class="fas fa-times-circle cross"></i>
    <% end %>
        <!-- <%# link_to "Delete -", memo, method: :delete, remote: true %> -->
        <p><%= memo.content%></p> <span> <%= memo.created_at.strftime("%m/%d/%Y")%></span></li>
    <% end %>
  </ul>
</div>
