<div id="dates">
  <ul>
    <% current_user.key_dates.each do |date| %>
      <li data-keydateid ="<%=date.id%>" class="keydate-line">
        <%= date.date%>, <%= date.description %>
        <%= link_to date, method: :delete, remote: true do %>
          <i class="fas fa-times-circle cross"></i></li>
          <p class="line"></p>
        <% end %>
    <% end %>
  </ul>
</div>
